{
    "name": "cavin-kim/laravel-naming-convention-enforcer",
    "description": "A Laravel package to enforce naming conventions.",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\KPhiri\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\KPhiri\\KPhiriServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cavin-kim/laravel-naming-convention-enforcer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}